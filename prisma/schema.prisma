generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model bookmarks {
  id         Int       @id(map: "bookmarks_pk") @default(autoincrement())
  title      String?   @db.VarChar
  url        String?
  created_at DateTime? @default(now()) @db.Timestamp(6)

  @@map("bookmarks") // ðŸ‘ˆ tells Prisma this maps to the "bookmarks" table
}
